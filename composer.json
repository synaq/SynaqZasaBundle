{
    "name": "synaq/zasa-bundle",
    "type": "symfony-bundle",
    "description": "A Symfony2 wrapper for the Zimbra Soap Admin API (ZASA)",
    "keywords": ["API","Zimbra","Symfony2"],
    "homepage": "https://github.com/synaq/SynaqZasaBundle",
    "authors": [
        {
            "name": "Nic Parry",
            "email": "nicjparry@gmail.com"
        },
        {
            "name": "Willem Viljoen",
            "email": "willemv@synaq.com"
        }
    ],
    "minimum-stability": "stable",
    "require": {
        "php": "^5.4|^7",
        "ext-curl": "*",
        "symfony/framework-bundle": "^2.5|^3.4",
        "synaq/curl-bundle": "^1.0"
    },
    "require-dev": {
	    "phpunit/phpunit": "^4.8|^5",
	    "mockery/mockery": "^0.9.5"
    },
    "autoload": {
        "psr-4": {
            "Synaq\\ZasaBundle\\Connector\\": "Connector",
            "Synaq\\ZasaBundle\\Exception\\" : "Synaq/ZasaBundle/Exception",
            "Synaq\\ZasaBundle\\Tests\\Connector\\" : "Tests/Connector"
        }
    }
}
